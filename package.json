{
  "name": "winkelzoeker-api",
  "version": "1.0.0",
  "description": "WinkelZoeker backend API",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules/ reports/ .serverless/ .webpack/ dist/ ./coverage",
    "build": "tsc",
    "build:prod": "npm clean; npm i; tsc",
    "test:all": "jest",
    "test:unit": "DEFAULT_ENVIRONMENT=DEV jest test/unit/**",
    "test:coverage": "jest test/unit/** --coverage",
    "start:dev": "nodemon --config nodemon.json ./src/adapters/controllers/rest-api/restify/v1/index.ts",
    "start:restify": "npm run build; node ./dist/src/adapters/controllers/rest-api/restify/v1/index.js",
    "start:restify:dev": "nodemon --config nodemon.json ./src/adapters/controllers/rest-api/restify/v1/index.ts",
    "start:restify:clean": "npm run build:prod; node ./dist/src/adapters/controllers/rest-api/restify/v1/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WinkelZoeker/winkelzoeker-api.git"
  },
  "keywords": [
    "search",
    "api"
  ],
  "author": "Rodrigo de Souza <rsouza01@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WinkelZoeker/winkelzoeker-api/issues"
  },
  "homepage": "https://github.com/WinkelZoeker/winkelzoeker-api#readme",
  "dependencies": {
    "haversine-distance": "^1.2.1",
    "http-status-codes": "^2.1.4",
    "mongoose": "^5.11.13",
    "restify": "^8.5.1",
    "restify-errors": "^8.0.2",
    "restify-plugins": "^1.6.0",
    "server-health": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/restify-errors": "^4.3.3",
    "@types/restify-plugins": "^1.5.12",
    "@types/source-map-support": "^0.5.3",
    "dotenv-extended": "^2.9.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "onchange": "^7.1.0",
    "serve": "^11.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yaml-jest": "^1.0.5"
  }
}
